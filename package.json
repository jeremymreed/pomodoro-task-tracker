{
  "name": "pomodoro-task-tracker",
  "version": "0.10.0",
  "description": "Task Tracking using the Pomodoro technique",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/jeremymreed/pomodoro-task-tracker"
  },
  "main": "build/main.js",
  "scripts": {
    "rebuild-usb": "electron-builder install-app-deps",
    "start": "electron ./",
    "start-dev": "electron ./ pomodoro-task-tracker-dev",
    "build": "npm run clean && npm run lint && npm run compile && npm run babel && npm run copy-static",
    "lint": "eslint ./src",
    "compile": "tsc -p ./",
    "babel": "babel build/ --out-dir build/ --source-maps",
    "copy-static": "cp static/* build/",
    "seed": "node build/scripts/seed.js",
    "seed-dev": "node build/scripts/seed.js pomodoro-task-tracker-dev",
    "test": "mocha -r esm tests/**/*.spec.js",
    "clean": "rm -rf dist/ build/",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Jeremy M. Reed <reeje76@gmail.com> (https://thetechforest.com)",
  "license": "GPLv2",
  "build": {
    "appId": "com.thetechforest.pomodoro-task-tracker",
    "files": [
      "./build/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "pacman"
        },
        {
          "target": "AppImage"
        },
        {
          "target": "snap"
        },
        {
          "target": "deb"
        },
        {
          "target": "rpm"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@types/electron-settings": "^3.1.1",
    "@types/humanize-duration": "^3.18.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chai": "^4.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.8.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.0",
    "eslint-plugin-react-hooks": "^4.1.2",
    "esm": "^3.2.25",
    "mocha": "^8.1.1",
    "prettier": "2.1.2",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "electron-settings": "^4.0.2",
    "env-paths": "^2.2.0",
    "formik": "^2.1.5",
    "humanize-duration": "^3.23.1",
    "moment": "^2.27.0",
    "node-luxafor2": "^1.0.3",
    "pouchdb": "^7.2.2",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.0"
  }
}