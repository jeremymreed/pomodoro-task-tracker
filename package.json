{
  "name": "pomodoro-task-tracker",
  "version": "0.9.0",
  "description": "Task Tracking using the Pomodoro technique",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/jeremymreed/pomodoro-task-tracker"
  },
  "main": "build/main.js",
  "scripts": {
    "start": "electron ./",
    "build": "npm run clean && npm run build:js && npm run copy-static",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".js,.jsx,.ts,.tsx\" --source-maps inline",
    "check-types": "tsc --noEmit -p .",
    "copy-static": "cp static/* build/",
    "installDB": "node build/scripts/install.js",
    "seed": "node build/scripts/seed.js",
    "test": "mocha -r esm tests/**/*.spec.js",
    "clean": "rm -rf dist/ build/ data/pomodoro-task-tracker*",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "author": "Jeremy M. Reed <reeje76@gmail.com> (https://thetechforest.com)",
  "license": "GPLv2",
  "build": {
    "appId": "com.thetechforest.pomodoro-task-tracker",
    "files": [
      "./build/**/*"
    ],
    "linux": {
      "target": [
        {
          "target": "pacman"
        },
        {
          "target": "AppImage"
        },
        {
          "target": "snap"
        },
        {
          "target": "deb"
        },
        {
          "target": "rpm"
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@types/electron-settings": "^3.1.1",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/uuid": "^8.3.0",
    "chai": "^4.2.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.8.0",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "esm": "^3.2.25",
    "mocha": "^8.1.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "electron-settings": "^4.0.2",
    "formik": "^2.1.5",
    "humanize-duration": "^3.23.1",
    "moment": "^2.27.0",
    "node-luxafor2": "^1.0.3",
    "pouchdb": "^7.2.2",
    "pouchdb-debug": "^7.2.1",
    "pouchdb-find": "^7.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7",
    "uuid": "^8.3.0"
  }
}
